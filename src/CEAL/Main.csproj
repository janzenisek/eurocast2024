<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <AssemblyName>CEAL.$(MSBuildProjectName)</AssemblyName>
    <RootNamespace>CEAL.$(MSBuildProjectName.Replace(" ", "_"))</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="DAT.Communication" Version="1.0.0" />
    <PackageReference Include="DAT.Configuration" Version="1.0.0" />
    <PackageReference Include="DAT.Utils" Version="1.0.0" />
  </ItemGroup>

</Project>
